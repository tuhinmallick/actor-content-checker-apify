{
  "title": "Content checker input",
  "type": "object",
  "schemaVersion": 1,
  "required": ["urls"],
  "properties": {
      "urls": {
          "title": "URLs to monitor",
          "type": "array",
          "description": "Array of URLs to monitor for content changes",
          "prefill": [
              {
                  "url": "https://www.apify.com/change-log",
                  "contentSelector": "article"
              }
          ],
          "items": {
              "type": "string",
              "enum": [
                  "https://www.apify.com/change-log",
                  "https://example.com"
              ]
          },
          "editor": "select"
      }
  }
}